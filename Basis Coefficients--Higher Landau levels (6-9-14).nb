Cell[BoxData[
 RowBox[{
  RowBox[{"Vmn", "[", 
   RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Assuming", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"k", "\[Element]", "Reals"}], "&&", 
      RowBox[{"k", ">", "0"}], "&&", 
      RowBox[{"d", "\[Element]", "Reals"}], "&&", 
      RowBox[{"d", "\[GreaterEqual]", "0"}], "&&", 
      RowBox[{"l", "\[Element]", "Reals"}], "&&", 
      RowBox[{"l", ">", "0"}]}], ",", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}]], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", 
        RowBox[{"2", "\[Pi]"}]], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
        RowBox[{
         FractionBox["1", "k"], 
         RowBox[{"Exp", "[", 
          RowBox[{"-", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"k", " ", "l"}], ")"}], "2"]}], "]"}], 
         RowBox[{"LaguerreL", "[", 
          RowBox[{"m", ",", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"k", " ", "l"}], ")"}], "2"]}], "]"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"LaguerreL", "[", 
            RowBox[{"n", ",", 
             FractionBox[
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"k", " ", "l"}], ")"}], "2"], "2"]}], "]"}], ")"}], 
          "2"], "k", 
         RowBox[{"\[DifferentialD]", "k"}], 
         RowBox[{"\[DifferentialD]", "\[Theta]"}]}]}]}]}]}], "]"}], "//", 
   "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.5886296744262505`*^9, 3.5886297056252155`*^9}, {
  3.589753712563592*^9, 3.589753712897636*^9}, {3.5905124836899757`*^9, 
