Cell[CellGroupData[{

Cell["\<\
Find the effective potential for the n=0 level that corresponds to a V(k) at \
a higher n:\
\>", "Section",
 CellChangeTimes->{{3.611349256890082*^9, 3.611349259466031*^9}, {
   3.6113493332150793`*^9, 3.611349374265621*^9}, 3.611349541681972*^9, 
   3.611354406500402*^9},
 Background->RGBColor[0.88, 1, 0.88]],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["V", "m"], "0"], "(", 
     RowBox[{
      FractionBox["1", "k"], "+", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "0"}], "4"], 
       RowBox[{
        SubscriptBox["c", "i"], 
        RowBox[{
         SubscriptBox["f", "i"], "(", "k", ")"}]}]}]}], ")"}], "=", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["V", "m"], "n"], "(", 
     RowBox[{"V", "(", "k", ")"}], ")"}]}], TraditionalForm]]],
 ".\n\nWe let m run from 0 to 4 so that there will be 5 equations with 5 \
unknowns (",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "0"], TraditionalForm]]],
 " to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "4"], TraditionalForm]]],
 "):\n\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["V", "0"], "0"], "(", 
     RowBox[{
      FractionBox["1", "k"], "+", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "0"}], "4"], 
       RowBox[{
        SubscriptBox["c", "i"], 
        RowBox[{
         SubscriptBox["f", "i"], "(", "k", ")"}]}]}]}], ")"}], "=", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["V", "0"], "n"], "(", 
     RowBox[{"V", "(", "k", ")"}], ")"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t\t", "\[VerticalEllipsis]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["V", "4"], "0"], "(", 
     RowBox[{
      FractionBox["1", "k"], "+", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "0"}], "4"], 
       RowBox[{
        SubscriptBox["c", "i"], 
        RowBox[{
         SubscriptBox["f", "i"], "(", "k", ")"}]}]}]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       SubscriptBox["V", "4"], "n"], "(", 
      RowBox[{"V", "(", "k", ")"}], ")"}], " ", "."}]}], TraditionalForm]}]],
 "\n\nWe can thus set up a matrix to solve for the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]]],
 ".  The classical term, i.e. the first term in the effective potential, is \
moved to the right side so that the left side is composed entirely of the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]]],
 " terms.  \n\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["c", "0"], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["V", "0"], "0"], "(", 
       RowBox[{
        SubscriptBox["f", "0"], "(", "k", ")"}], ")"}]}], "+", 
     RowBox[{
      SubscriptBox["c", "1"], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["V", "0"], "0"], "(", 
       RowBox[{
        SubscriptBox["f", "1"], "(", "k", ")"}], ")"}], "\[Ellipsis]", 
      SubscriptBox["c", "4"], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["V", "0"], "0"], "(", 
       RowBox[{
        SubscriptBox["f", "4"], "(", "k", ")"}], ")"}]}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       SubscriptBox["V", "0"], "n"], "(", 
      RowBox[{"V", "(", "k", ")"}], ")"}], "-", 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["V", "0"], "0"], "(", 
      FractionBox["1", "k"], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t\t\t\t", "\[VerticalEllipsis]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["c", "0"], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["V", "4"], "0"], "(", 
       RowBox[{
        SubscriptBox["f", "0"], "(", "k", ")"}], ")"}]}], "+", 
     RowBox[{
      SubscriptBox["c", "1"], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["V", "4"], "0"], "(", 
       RowBox[{
        SubscriptBox["f", "1"], "(", "k", ")"}], ")"}], "\[Ellipsis]", 
      SubscriptBox["c", "4"], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["V", "4"], "0"], "(", 
       RowBox[{
        SubscriptBox["f", "4"], "(", "k", ")"}], ")"}]}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       SubscriptBox["V", "4"], "n"], "(", 
      RowBox[{"V", "(", "k", ")"}], ")"}], "-", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        SubscriptBox["V", "4"], "0"], "(", 
       FractionBox["1", "k"], ")"}], " ", "."}]}]}], TraditionalForm]}]],
 "\n\nNow this can be written in the matrix form:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["A",
      FontWeight->"Bold"], " ", 
     OverscriptBox["c", "\[RightVector]"]}], "=", 
    OverscriptBox["v", "\[RightVector]"]}], TraditionalForm]]],
 "\n\nwhere:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["A",
     FontWeight->"Bold"], 
    StyleBox["=",
     FontWeight->"Bold"], 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SuperscriptBox[
          SubscriptBox["V", "0"], "0"], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["f", "0"], 
           RowBox[{"(", "k", ")"}]}], ")"}]}], "\[CenterEllipsis]", 
        "\[CenterEllipsis]", 
        RowBox[{
         SuperscriptBox[
          SubscriptBox["V", "0"], "0"], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["f", "4"], 
           RowBox[{"(", "k", ")"}]}], ")"}]}]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
        "\[VerticalEllipsis]"},
       {
        RowBox[{
         SuperscriptBox[
          SubscriptBox["V", "4"], "0"], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["f", "0"], 
           RowBox[{"(", "k", ")"}]}], ")"}]}], "\[CenterEllipsis]", 
        "\[CenterEllipsis]", 
        RowBox[{
         SuperscriptBox[
          SubscriptBox["V", "4"], "0"], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["f", "4"], 
           RowBox[{"(", "k", ")"}]}], ")"}]}]}
      }], ")"}]}], TraditionalForm]]],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["c", "\[RightVector]"], "=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["c", "0"]},
       {"\[VerticalEllipsis]"},
       {
        SubscriptBox["c", "4"]}
      }], ")"}]}], TraditionalForm]]],
 "\n \n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["v", "\[RightVector]"], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         RowBox[{
          SuperscriptBox[
           SubscriptBox["V", "0"], "n"], "(", 
          RowBox[{"V", "(", "k", ")"}], ")"}], "-", 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["V", "0"], "0"], 
          RowBox[{"(", 
           FractionBox["1", "k"], ")"}]}]}]},
       {"\[VerticalEllipsis]"},
       {
        RowBox[{
         RowBox[{
          SuperscriptBox[
           SubscriptBox["V", "4"], "n"], "(", 
          RowBox[{"V", "(", "k", ")"}], ")"}], "-", 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["V", "4"], "0"], 
          RowBox[{"(", 
           FractionBox["1", "k"], ")"}]}]}]}
      }], ")"}]}], TraditionalForm]]],
 " ."
}], "Text",
