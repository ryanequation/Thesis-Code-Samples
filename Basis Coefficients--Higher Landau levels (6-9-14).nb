Cell[TextData[{
 "We find the constants ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]]],
 " of the effective potential by equating the Haldane pseudopotential of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["V", "eff"], "(", "k", ")"}], "=", 
    RowBox[{
     FractionBox["1", "k"], "+", 
     RowBox[{
      FractionBox["1", "l"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "0"}], "imax"], 
       RowBox[{
        SubscriptBox["c", "i"], 
        RowBox[{
         SubscriptBox["f", "i"], "(", "k", ")"}]}]}]}]}]}], TraditionalForm]]],
 "  to the Haldane pseudopotential of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["V", "ZDS"], "(", "k", ")"}], TraditionalForm]]],
 ".\n\nReference:  \[OpenCurlyDoubleQuote]Frame potential--Haldane \
pseudopotential (9-19-13).nb\[CloseCurlyDoubleQuote]"
}], "Text",
 CellChangeTimes->{{3.588632032532953*^9, 3.588632077144622*^9}, {
  3.588632232394354*^9, 3.5886322666317024`*^9}, {3.591797273847848*^9, 
  3.5917972742939034`*^9}, {3.6088483902903*^9, 3.608848391243174*^9}, {
  3.6088484327705107`*^9, 3.6088484342647247`*^9}, {3.608939537675189*^9, 
  3.608939540678912*^9}}],

Cell[TextData[{
 "The effective potential is:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"V", "(", "r", ")"}], "eff"], "=", 
    RowBox[{
     FractionBox["1", "r"], "+", 
     RowBox[{
      FractionBox["1", "l"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "0"}], "imax"], 
       RowBox[{
        SubscriptBox["c", "i"], " ", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["r", "l"], ")"}], "i"], 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"-", 
          SuperscriptBox[
           RowBox[{"(", 
            FractionBox["r", "l"], ")"}], "2"]}]]}]}]}]}]}], 
   TraditionalForm]]],
 ".  The placement of the l\[CloseCurlyQuote]s ensures that the second part \
has units of 1/length.\nTake the 2-D Fourier transform using the formula (see \
\[OpenCurlyDoubleQuote]2-D Fourier transform integral form.nb\
\[CloseCurlyDoubleQuote], \[OpenCurlyDoubleQuote]Higher Mathematics for\n\
Physics and Engineering\[CloseCurlyDoubleQuote] by Shima, Nakayama and p. \
690, 680; Arfken and Weber) \n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"F", "(", 
     OverscriptBox["k", "\[RightVector]"], ")"}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{"r", " ", 
      RowBox[{"f", "(", "r", ")"}], " ", 
      RowBox[{
       SubscriptBox["J", "0"], "(", 
       RowBox[{"k", " ", "r"}], ")"}], 
      RowBox[{"\[DifferentialD]", "r"}]}]}]}], TraditionalForm]]],
 " \n\nso that:\n\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"V", "(", "k", ")"}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{"r", " ", 
      RowBox[{"V", "(", "r", ")"}], " ", 
      RowBox[{
       SubscriptBox["J", "0"], "(", 
       RowBox[{"k", " ", "r"}], ")"}], 
      RowBox[{"\[DifferentialD]", "r"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{"r", " ", 
      RowBox[{"(", 
       RowBox[{
        FractionBox["1", "r"], "+", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"i", "=", "0"}], "imax"], 
         RowBox[{
          SubscriptBox["c", "i"], 
          SuperscriptBox["r", "i"], 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"-", 
            SuperscriptBox["r", "2"]}]]}]}]}], ")"}], " ", 
      RowBox[{
       SubscriptBox["J", "0"], "(", 
       RowBox[{"k", " ", "r"}], ")"}], 
      RowBox[{"\[DifferentialD]", "r"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
      RowBox[{
       RowBox[{
        SubscriptBox["J", "0"], "(", 
        RowBox[{"k", " ", "r"}], ")"}], 
       RowBox[{"\[DifferentialD]", "r"}]}]}], "+", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}], "imax"], 
      RowBox[{
       SubscriptBox["c", "i"], 
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
        RowBox[{
         SuperscriptBox["r", 
          RowBox[{"i", "+", "1"}]], 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", 
           SuperscriptBox["r", "2"]}]], " ", 
